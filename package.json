{
  "name": "duck_tracks",
  "version": "1.0.0",
  "description": "A data crowdsourcing application designed to aggregate data on how ducks are fed around the world.",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "server": "nodemon server",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\" ",
    "build": "npm i && cd client && npm install && npm run build && cd .."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arranw/duck_tracks.git"
  },
  "author": "Arran Woodruff",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/arranw/duck_tracks/issues"
  },
  "homepage": "https://github.com/arranw/duck_tracks#readme",
  "dependencies": {
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "config": "^3.3.1",
    "express": "^4.17.1",
    "mysql2": "^2.1.0",
    "react-redux": "^7.2.1",
    "redux": "^4.0.5",
    "sequelize": "^6.3.3",
    "styled-components": "^5.1.1"
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "jest": "^24.9.0",
    "nodemon": "^2.0.4",
    "supertest": "^4.0.2"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "testPathIgnorePatterns": [
      "/client/"
    ]
  }
}
